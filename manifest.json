{
    "background": {
        "scripts": [ "background.js" ]
     },
   "browser_action": {
      "default_icon": "icon48.png",
      "default_title": "Autotask - Only with tabs."
   },
   "description": "Forces Chrome to open new tabs instead of endless popup Windows in Autotask",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Autotask - Only with tabs.",
   "short_name": "AutotaskTabs",
   "permissions": [ "tabs", "webNavigation", "contentSettings" ],
   "version": "0.1"
}
